{
  "name": "babymarkt/composer-cleaner-plugin",
  "type": "composer-plugin",
  "description": "A composer plugin to clean up your project directory",
  "keywords": [
    "composer",
    "plugin",
    "directories",
    "files",
    "cleanup"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Niklas Funke",
      "email": "funke@babymarkt.de"
    }
  ],
  "extra": {
    "class": "Babymarkt\\Composer\\Cleaner\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Babymarkt\\Composer\\Cleaner\\": "src/Cleaner"
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "require": {
    "php": ">=5.6 || ^7",
    "composer-plugin-api": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "mikey179/vfsstream": "~1",
    "composer/composer": "^1.10",
    "symfony/framework-bundle": "*"
  }
}
